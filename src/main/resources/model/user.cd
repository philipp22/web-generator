@Domain
@NoRest
domain User {
	@GetOneBy
	@Readonly
	String username;
	String email;
	String passwordHash;
	@Readonly
	long registrationDate;
	String profilePicture;
	String displayName;
	@ManyToMany
	List<User> friends;

	rest {
		get: ReducedUser;
	}
}

@DTO
domain ReducedUser {
	String username;
	String profilePicture;
	String displayName;
}