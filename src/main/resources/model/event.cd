@Domain
domain Event {
	String title;
	@Text
	String description;
	String location;
	long start;
	@Text
	String image ?;
	@OneToMany
	List<User> accepted;
	@OneToMany
	List<User> declined;
	@OneToMany
	List<User> ignored;
}

@Domain
domain Location {
	String name;
	String street;
	String houseNumber;
	String postalCode;
	String town;
	String addition;
}
